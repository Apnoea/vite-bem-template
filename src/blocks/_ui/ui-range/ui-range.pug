mixin ui-range(mods)
  -
    const allMods = mods ? mods.split(',').map(mod => mod.trim()[0] === '-' ? `ui-range${mod.trim()}` : `${mod.trim()}__ui-range`).join(' ').trim() : ''

  .ui-range(class=allMods)&attributes(attributes)
    .ui-range__value
      .ui-range__lower
      .ui-range__upper
    .ui-range__body
    block
